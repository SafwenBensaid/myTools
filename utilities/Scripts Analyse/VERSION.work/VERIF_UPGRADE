PACK="TAF-$1"

TABLE=`rsh -l t24inv 172.28.70.15 ls  /APP/biatinv/bnk/bnk.run/PACK.TAF/$PACK|cut -d- -f1`

OBJET=`rsh -l t24inv 172.28.70.15 ls  /APP/biatinv/bnk/bnk.run/PACK.TAF/$PACK`

VAR=`rsh -l t24inv 172.28.70.15 ls  /APP/biatinv/bnk/bnk.run/PACK.TAF/$PACK|cut -d- -f2-|grep  "^I_"`

NBR=`rsh -l t24inv 172.28.70.15 ls -l /APP/biatinv/bnk/bnk.run/PACK.TAF/$PACK|wc -l`


NBRR=$((NBR - 1))




echo -e "#_#$NBRR#_#"

# for TAB in  $TABLE;
# do
