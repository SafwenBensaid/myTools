# done

#for VV in  $VAR;
#do
 #if [ "$VV" != "" ];then
#echo " il y a un I_ dans le pack : $VV ";
#fi;
#done


for lig in  `cat ContrainteIntg`
do

for ii in $TABLE;
do
PP=`echo $ii |grep $lig|sed "s/ //g"`;

# echo "--$ii - $lig - $PP --";

 if [ "$PP" != "" ];then 
echo -e 'O_O'$PP'O_O'; 
fi; 
done

done

